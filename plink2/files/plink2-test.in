#!/bin/sh -e

if ! which plink; then
    cat << EOM

You must install both plink and plink2 before running the plink2 tests.

EOM
    exit 1
fi

if [ -e plink2-Tests ]; then
    cat << EOM

plink2-Tests already exits.  Remove it first if you want to run $0.

EOM
    exit 1
fi

cp -R %%DATADIR%%/Tests plink2-Tests
cd plink2-Tests
./run_tests.sh
