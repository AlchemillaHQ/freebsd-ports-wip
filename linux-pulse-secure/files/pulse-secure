#!/bin/sh -e

PATH=${PATH}:/compat/linux/usr/local/nc
export PATH

/usr/local/linux-sun-jdk1.7.0/bin/java \
    -jar /compat/linux/usr/local/nc/NC.jar \
    -h vpn.uwm.edu \
    -u bacon 

# Usage: java -cp NC.jar NC -h host -u user -p passwd -f cert_file \
#   [-r realm] [-l log_level] [-L log_level] [-U sign_in_url] [-y proxy] \
#   [-z proxy_port] [-s proxy_user] [-a proxy_password] [-d proxy_domain] \
#   [-n start_script] [-t end_script]

